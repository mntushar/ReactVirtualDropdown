  "bugs": {
    "url": "https://github.com/mntushar/ReactVirtualDropdown/issues"
  },
  "homepage": "https://github.com/mntushar/ReactVirtualDropdown#readme",
  "devDependencies": {
    "@types/react": "^19.0.10",
    "ts-loader": "^9.5.2",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  }
